{% extends "base" %}

{% block content %}
<div>
    <a href="/add" class="btn">clone a project</a>
</div>
<div class="others">
    <label for="search"></label><input type="search" name="search" class="search" id="search"
        placeholder="Search a project" autofocus="autofocus">
    <ul id="myList" class="columns-2">
        {% for k, v in projects %}
        <li><a href="/manage/{{k}}" class="btn">{{k}}</a></li>
        {% endfor %}
    </ul>
</div>
{% endblock content %}